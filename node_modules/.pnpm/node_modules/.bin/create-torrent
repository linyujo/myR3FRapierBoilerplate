#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules/create-torrent/bin/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules/create-torrent/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules/create-torrent/bin/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules/create-torrent/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/create-torrent@5.0.9/node_modules:/Users/user/Documents/frontendWorkspace/myR3FRapierBoilerplate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-torrent/bin/cmd.js" "$@"
else
  exec node  "$basedir/../create-torrent/bin/cmd.js" "$@"
fi
